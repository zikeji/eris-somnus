{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb-typescript-prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "project": "./tsconfig.json"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
        "react": {
            "version": "999.999.999"
        }
    },
    "rules": {
        "lines-between-class-members": 0,
        "import/prefer-default-export": 0
    }
}